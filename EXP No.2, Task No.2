#include <stdio.h> 
 
int main() 
{ 
    int start, end, sum = 0; 
 
    printf("Enter the starting number: "); 
    scanf("%d", &start); 
    printf("Enter the ending number: "); 
    scanf("%d", &end); 
 
    if (start % 2 == 0) 
    { 
        start++; 
    } 
 
    for (int i = start; i <= end; i += 2) 
    { 
        sum += i; 
    } 
 
    printf("The sum of all odd numbers between %d and %d is: %d\n", start, end, sum); 
 
    return 0; 
} 

  Output:
Enter the starting number: 6
Enter the ending number: 9
The sum of all odd numbers between 7 and 9 is: 16
